{
  "name": "pokeshop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "node server/server.js",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier . --write",
    "deploy:frontend": "powershell -ExecutionPolicy Bypass -File scripts/deploy-frontend.ps1",
    "deploy:backend": "powershell -ExecutionPolicy Bypass -File scripts/deploy-backend.ps1",
    "deploy": "npm run deploy:backend && npm run deploy:frontend",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "serverless:local": "serverless offline",
    "populate-cache": "node scripts/populate-cache.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.11.0",
    "@aws-sdk/client-s3": "^3.975.0",
    "@iconify/react": "^6.0.2",
    "@prisma/client": "^6.19.2",
    "@tanstack/react-query": "^5.90.19",
    "@vendia/serverless-express": "^4.12.6",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-jwt": "^8.5.1",
    "jwks-rsa": "^3.2.0",
    "multer": "^1.4.5-lts.1",
    "prisma": "^6.19.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.12.0",
    "stripe": "^20.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.23",
    "esbuild": "^0.24.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "serverless-esbuild": "^1.54.0",
    "serverless-offline": "^14.4.0",
    "tailwindcss": "^3.4.19",
    "vite": "^7.1.7",
    "vitest": "^4.0.18"
  }
}
